{
  "name": "artillery",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "dev": "next dev -p 4281",
    "build": "next build -p 4281",
    "start": "next start -p 4281",
    "lint": "next lint",
    "pm2-dev": "pm2 start npm --name \"Ronatec Next.js Dev\" -- run dev",
    "pm2-dev-stop": "pm2 stop \"Ronatec Next.js Dev\" && pm2 delete \"Ronatec Next.js Dev\"",
    "pm2-dev-restart": "pm2 restart \"Ronatec Next.js Dev\"",
    "pm2-start": "pm2 start npm --name \"Ronatec Next.js Production\" -- run start",
    "pm2-stop": "pm2 stop \"Ronatec Next.js Production\" && pm2 delete \"Ronatec Next.js Production\"",
    "pm2-restart": "pm2 restart \"Ronatec Next.js Production\"",
    "build:css": "postcss assets/main.css -o assets/main_browser.css",
    "schema-gen": "graphql-codegen --config codegen.yml",
    "test": "jest --watch \".*\\.test\\.ts*\"",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "next": "12.1.4",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.20.2",
    "@testing-library/dom": "^8.12.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^14.0.4",
    "@types/jest": "^27.4.1",
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@types/validator": "^13.7.2",
    "axios": "^0.26.1",
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.4",
    "eslint-plugin-testing-library": "^5.2.0",
    "jest": "^27.5.1",
    "msw": "^0.39.2",
    "ts-jest": "^27.1.4",
    "typescript": "4.6.3"
  },
  "description": "Frontend for ARTillery.design",
  "main": "index.js",
  "repository": "git@github.com:RevNelson/artillery-website.git",
  "author": "Michael Nelson <revnelson@gmail.com>",
  "license": "MIT"
}
